default namespace = "http://novell.com/package/metadata/suse/patterns"

# repodata/patterns.xml schema
#
# This is a SUSE extension to the RPM Metadata
# http://en.opensuse.org/Libzypp/Metadata/repomd/patterns
# http://en.opensuse.org/Patterns

include "pattern.rnc" {
  start = element-patterns
}

element-patterns = element patterns {
  attribute count { xsd:nonNegativeInteger }?,
  element-pattern+
}
